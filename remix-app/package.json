{
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "remix build",
    "dev": "remix build && cross-env NODE_ENV=development run-p --print-label --race \"dev:*\"",
    "dev:server": "nodemon server.mjs --watch server.mjs",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node server.mjs",
    "lint": "eslint --ext js,ts,tsx .",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@remix-run/express": "^1.7.2",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "chalk": "^5.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.5",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "@types/nodemon": "^1.19.2",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.23.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  },
  "prettier": "@itsmapleleaf/configs/prettier"
}
