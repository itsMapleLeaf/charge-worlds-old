    "ariakit": "2.0.0-next.41",
    "chalk": "^5.0.1",
    "clsx": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.3",
    "framer-motion": "^7.5.1",
    "isomorphic-ws": "^5.0.0",
    "lucide-react": "^0.91.0",
    "nanostores": "^0.7.0",
    "node-cron": "^3.0.2",
    "pusher": "5.1.1-beta",
    "pusher-js": "^7.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-expanding-textarea": "^2.3.6",
