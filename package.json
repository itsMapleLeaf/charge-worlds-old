    "@liveblocks/node": "^0.18.3",
    "@liveblocks/react": "^0.18.3",
    "@mdi/js": "^7.0.96",
    "@mdi/react": "^1.6.1",
    "@nanostores/react": "^0.4.0",
    "@prisma/client": "^4.4.0",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/server-runtime": "^1.7.2",
    "@remix-run/vercel": "^1.7.2",
