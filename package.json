{
  "type": "module",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepare": "typesync && prisma generate",
    "dev": "vite",
    "dev:server": "nodemon --exec tsx --ignore \"vite.config.ts.*\" server.ts",
    "dev:docker": "docker-compose up",
    "build": "vite build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "eslint --ext js,ts,tsx .",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fontsource/oswald": "^4.5.10",
    "@fontsource/rubik": "^4.5.11",
    "@liveblocks/client": "^0.18.2",
    "@liveblocks/react": "^0.18.2",
    "@prisma/client": "^4.3.1",
    "@tanstack/react-query": "^4.3.9",
    "@trpc/client": "10.0.0-proxy-beta.5",
    "@trpc/react": "10.0.0-proxy-beta.5",
    "@trpc/server": "10.0.0-proxy-beta.5",
    "chalk": "^5.0.1",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "tsx": "^3.9.0",
    "ws": "^8.9.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.5",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "@types/nodemon": "^1.19.2",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/ws": "^8.5.3",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.23.1",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3",
    "typesync": "^0.9.2",
    "vite": "^3.1.3"
